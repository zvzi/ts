-- Scripts:

local function HMODCD_fake_script() -- CloseButton.Script 
	local script = Instance.new('Script', CloseButton)

	frame = script.Parent.Parent
	button = script.Parent
	
	button.MouseButton1Click:connect(function()
		frame.Visible = false
	end)
	
	
end
coroutine.wrap(HMODCD_fake_script)()
local function GDWB_fake_script() -- fov.LocalScript 
	local script = Instance.new('LocalScript', fov)

	script.Parent.MouseButton1Click:Connect(function()
	
		game.Workspace.CurrentCamera.FieldOfView  = 100
	end)
end
coroutine.wrap(GDWB_fake_script)()
local function IRPSLRB_fake_script() -- resetkey.LocalScript 
	local script = Instance.new('LocalScript', resetkey)

	script.Parent.MouseButton1Click:Connect(function()
	
		wait(2)
		print("reset by Kutsukie has loaded")
		game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(KeyPressed)
			if KeyPressed == "x" then
				game.Players.LocalPlayer.Character.Humanoid.Health = 0
			end
		end)
	end)
end
coroutine.wrap(IRPSLRB_fake_script)()
local function GAGK_fake_script() -- RedeemCodeOpen.Script 
	local script = Instance.new('Script', RedeemCodeOpen)

	print("Kutsukie's LGM has loaded")
	button = script.Parent
	frame = script.Parent.Parent.Parent.OpenTSKit.MainFrame
	
	button.MouseButton1Click:connect(function()
		frame.Visible = true
	end)
	
end
coroutine.wrap(GAGK_fake_script)()
